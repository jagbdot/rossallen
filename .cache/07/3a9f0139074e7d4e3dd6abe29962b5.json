 'swing',\n      links: 'internal'\n    }, options);\n\n\n    var fadePage = function ($link) {\n      $('body').fadeOut(settings.duration, settings.easing, function () {\n        window.location.href = $link;\n      });\n    };\n\n    if (settings.links === 'all') {\n      $('a').on('click', function (e) {\n        e.preventDefault();\n        var $link = $(this).attr('href');\n        fadePage($link);\n      });\n    } else if (settings.links === 'internal') {\n      $('a').on('click', function (e) {\n        var a = new RegExp('/' + window.location.host + '/');\n        if (a.test(this.href)) {\n          console.log(\"external link\");\n          e.preventDefault();\n          var $link = $(this).attr('href');\n          fadePage($link);\n        }\n      });\n    } else {\n      $('a' + settings.links).on('click', function (e) {\n        e.preventDefault();\n        var $link = $(this).attr('href');\n        fadePage($link);\n      });\n    }\n\n  };\n}(jQuery));"},"lineCount":null}},"error":null,"hash":"0c6ea8c543cb26522bbb0d6af286ff4c","cacheData":{"env":{}}}